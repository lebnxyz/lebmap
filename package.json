{
  "name": "lebanese",
  "version": "0.0.0",
  "description": "Interactive dialectal-feature map for Lebanese Arabic",
  "private": true,
  "scripts": {
    "build": "parcel build index.html -d docs/ --public-url ./",
    "dev": "parcel index.html",
    "prep": "SETLOCAL && FOR /F \"tokens=* USEBACKQ\" %g IN (`git lastmsg`) do (set \"COMMITMSG=%g\") && git reset HEAD~1 && npm run build && CALL git commit -am \"%COMMITMSG%\" && ENDLOCAL",
    "push": "npm run prep && git push",
    "pushf": "npm run prep && git push -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supposedly/lebanese.git"
  },
  "author": "Hadi Tarhini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supposedly/lebanese/issues"
  },
  "homepage": "https://github.com/supposedly/lebanese#readme",
  "dependencies": {
    "d3": "^5.12.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-json-url-loader": "^0.1.2"
  }
}
