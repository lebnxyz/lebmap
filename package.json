{
  "name": "leb",
  "version": "0.0.0",
  "description": "Interactive dialectal-feature map for Lebanese Arabic",
  "private": true,
  "scripts": {
    "build": "parcel build index.html -d docs/ --public-url ./",
    "dev": "parcel index.html",
    "prep": "SETLOCAL && FOR /F \"tokens=* USEBACKQ\" %g IN (`git lastmsg`) do (set \"COMMITMSG=%g\") && git reset HEAD~1 && del docs\\* /Q && npm run build && git add docs\\ && CALL git commit -am \"%COMMITMSG%\" && ENDLOCAL",
    "push": "npm run prep && git push",
    "pushf": "npm run prep && git push -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supposedly/leb.git"
  },
  "author": "Hadi Tarhini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supposedly/leb/issues"
  },
  "homepage": "https://github.com/supposedly/leb#readme",
  "dependencies": {
    "d3": "^5.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "browserslist": "^4.7.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-json-extended": "^0.1.1"
  },
  "browserslist": [
    "> .5% and last 2 versions",
    "> .5% in LB"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ]
    ]
  }
}
